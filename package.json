    "express-validator": "^6.14.2",
    "lodash": "^4.17.21",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.25.0",
    "sequelize-mig": "^3.1.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.1"
  }
